{
    "name": "phpcq/travis-configuration-check",
    "description": "Validation tool to ensure that the contents of a .travis.yml make sense.",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Schiffler",
            "email": "c.schiffler@cyberspectrum.de",
            "homepage": "http://www.cyberspectrum.de",
            "role": "Developer"
        },
        {
            "name": "Tristan Lins",
            "email":"tristan@lins.io",
            "homepage":"https://tristan.lins.io",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/phpcq/travis-configuration-check",
        "source": "https://github.com/phpcq/travis-configuration-check",
        "irc": "irc://irc.freenode.org/phpcq"
    },
    "autoload": {
        "psr-4": {
            "PhpCodeQuality\\TravisConfigurationCheck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpCodeQuality\\TravisConfigurationCheck\\Test\\": "tests"
        }
    },
    "require": {
        "php": "^5.3 || ^7.0",
        "composer/semver": "^1.2.0",
        "symfony/console": "^2.3 || ^3.0 || ^4.0",
        "symfony/yaml": "^2.3 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "phpcq/all-tasks": "^1.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev",
            "dev-develop": "1.3.x-dev"
        }
    },
    "bin": ["bin/check-travis-configuration.php"]
}
